{
	"name": "sveltekit-saas",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"setup:lemonsqueezy": "cross-env NODE_ENV=development tsx --tsconfig ./tsconfig.json ./src/scripts/setup-lemonsqueezy.ts",
		"setup:db": "",
		"setup": "pnpm run setup:lemonsqueezy",
		"db:studio": "drizzle-kit studio",
		"db:clear": "cross-env NODE_ENV=development tsx --tsconfig ./tsconfig.json ./src/scripts/db-clear.ts",
		"db:migrate": "cross-env NODE_ENV=development tsx --tsconfig ./tsconfig.json ./src/scripts/db-migrate.ts",
		"db:generate": "cross-env NODE_ENV=development drizzle-kit generate --config=drizzle.config.ts",
		"db:push": "cross-env NODE_ENV=development drizzle-kit push --config=drizzle.config.ts",
		"db:reset": "pnpm run db:clear && pnpm run db:push && pnpm run setup:db"
	},
